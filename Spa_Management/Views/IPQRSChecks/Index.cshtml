@model IEnumerable<Spa_Management.Models.tbIPQRSChecks>

@{
    ViewData["Title"] = "Index";
}<div class="panel panel-info ">
    <div class="panel-heading "><h4>IPQRS Checks</h4></div>
    <div class="panel-body">
        @*<p>
            <a asp-action="Create">Add New</a>
        </p>*@
        <table class="table table-hover table-responsive table-striped">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.inComeDetails)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.score)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.status)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.date)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.tbIndivuduals)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.inComeDetails)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.score)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.status)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.date)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.tbIndivuduals.FirstName)
                        </td>
                        <td>
                            @*<a asp-action="Edit" asp-route-id="@item.ipqrsGuid">Edit</a> |*@
                            <a asp-action="Details" asp-route-id="@item.ipqrsGuid">Details</a> |
                            <a asp-action="Delete" asp-route-id="@item.ipqrsGuid">Delete</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
