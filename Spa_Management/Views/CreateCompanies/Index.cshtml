@model IEnumerable<Spa_Management.Models.tbCompanies>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.registeredBy)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.compName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.pysicalLoc)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.postalAddress)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.PinNo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.RegCertNo)
