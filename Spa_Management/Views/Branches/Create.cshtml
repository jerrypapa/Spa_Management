@model Spa_Management.Models.tbBranches

@{
    ViewData["Title"] = "Create";
}

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="BankCode" class="control-label"></label>
                <select asp-for="BankCode" class="form-control" asp-items="ViewBag.BankCode">
                    <option>~ Please Select ~</option>
                </select>
            </div>
            <div class="form-group">
                <label asp-for="branchCode" class="control-label"></label>
                <input asp-for="branchCode" class="form-control" />
