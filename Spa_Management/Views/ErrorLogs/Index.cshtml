@model IEnumerable<Spa_Management.Models.tbErrorLogs>

@{
    ViewData["Title"] = "Index";
}

<section class="content">
    <div class="row">
        <div class="box">
            <div class="box-body">
                <div class="panel panel-info ">
                    <div class="panel-heading "><h4>Error Logs</h4></div>
                    <div class="panel-body">
                        <table id="example1" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    @*<th>
                                        @Html.DisplayNameFor(model => model.sessionId)
                                    </th>*@
                                    <th>
                                        @Html.DisplayNameFor(model => model.action)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Description)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.json)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.OpDate)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.errorMessage)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.innerExemption)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.modelValidation)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.attStatus)
                                    </th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        @*<td>
                            @Html.DisplayFor(modelItem => item.sessionId)
                        </td>*@
                                        <td>
                                            @Html.DisplayFor(modelItem => item.action)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Description)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.json)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.OpDate)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.errorMessage)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.innerExemption)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.modelValidation)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.attStatus)
                                        </td>
                                        <td>
                                            @*<a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                            <a asp-action="Details" asp-route-id="@item.Id">Details</a> |*@
                                            <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<script>
    $(function () {
        $("#example1").DataTable(
            {
                "ordering": false
            }
        );

    });
</script>

