@model Spa_Management.Models.tbBankBranches

@{
    ViewData["Title"] = "Edit";
}

<div class="row">
    <div class="col-sm-12 col-md-9 col-lg-6">
        <div class="panel panel-info ">
            <div class="panel-heading "><h4>Edit Branch</h4></div>
            <div class="panel-body">
                <form asp-action="Edit">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <input type="hidden" asp-for="BranchGuid" />
                    <div class="form-group">
                        <label asp-for="SystemBanksGuid" class="control-label"></label>
                        <select asp-for="SystemBanksGuid" class="form-control" asp-items="ViewBag.SystemBanksGuid"></select>
                        <span asp-validation-for="SystemBanksGuid" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="branchCode" class="control-label"></label>
                        <input asp-for="branchCode" class="form-control" />
                        <span asp-validation-for="branchCode" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="branchName" class="control-label"></label>
                        <input asp-for="branchName" class="form-control" />
                        <span asp-validation-for="branchName" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="regDate" class="control-label"></label>
                        <input asp-for="regDate" class="form-control" />
                        <span asp-validation-for="regDate" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="physicalLoc" class="control-label"></label>
                        <input asp-for="physicalLoc" class="form-control" />
                        <span asp-validation-for="physicalLoc" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="adrress" class="control-label"></label>
                        <input asp-for="adrress" class="form-control" />
                        <span asp-validation-for="adrress" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="status" class="control-label"></label>
                        <input asp-for="status" class="form-control" />
                        <span asp-validation-for="status" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="BankCollectionAccount" class="control-label"></label>
                        <input asp-for="BankCollectionAccount" class="form-control" />
                        <span asp-validation-for="BankCollectionAccount" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Save" class="btn btn-primary" />
                    </div>
                </form>
            </div>
        </div>

        <div>
            <a asp-action="Index">Back to List</a>
        </div>
    </div>
</div>
